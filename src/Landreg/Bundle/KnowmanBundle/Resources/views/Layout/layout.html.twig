<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">

    <title>{% block title %}{% if object.title %}{{ object.title }}{% endif %}{% endblock %}</title>

    {% block stylesheets %}
    <link href="{{ asset('bundles/acmedemo/css/bootstrap.min.css') }}" rel="stylesheet">
    {% endblock %}
</head>
<body class="container page">
{% block body %}
    {% block nav %}
    <nav class="navbar navbar-inverse page__nav" role="navigation">
        <div class="container-fluid">
            {{ knp_menu_render('simple', {'template': 'LandregKnowmanBundle:Menu:bootstrap.html.twig', 'currentClass': 'active'}) }}

        </div>
    </nav>
    {% endblock %}
    <div class="page__main">
        {% block main %}{% endblock %}
    </div>
    {% block footer %}{% endblock %}
{% endblock %}
</body>
</html>
